    "angular2": "2.0.0-beta.0",
    "bootstrap": "^3.3.6",
    "del": "^2.2.0",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.3",
    "gulp": "^3.9.0",
    "gulp-useref": "^3.0.5",
    "jspm": "^0.16.24",
    "reflect-metadata": "0.1.2",
